one_seq_rev = one_seq %>% {. - max(.)} %>% {. * -1} %>% rev()
# Scale sequence to n
scaled_one_seq = one_seq_rev %>% {. * n} %>% round() %>% {. + 1} %>% c(0,.) %>% {. / (n + 1)}
return(scaled_one_seq)
}
